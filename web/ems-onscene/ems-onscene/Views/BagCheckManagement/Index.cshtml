@model IEnumerable<ems_onscene.Models.EntityModels.EMSBagCheck>

@{
    ViewBag.Title = "Index";
}
<p class="float-end">
    @Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-default" })
</p>

<h2>Bag Check Management</h2>
<div class="clearfix"></div>
<table class="table">
    <tr>
        <th>
            <label>Bag Check Title</label>
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.BagCheckTitle, "Details", new { id = item.Id })
            </td>
            <td>
                @Html.ActionLink("Delete", "Delete", new { id = item.Id }, new { @class = "btn btn-sm btn-danger" })
            </td>
        </tr>
    }
</table>
